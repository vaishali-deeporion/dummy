{
  "name": "qa-backend",
  "version": "1.0.0",
  "description": "Visual Regression Testing Backend Server with Session-based Browser Isolation",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "prod": "NODE_ENV=production node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t backstop-backend:latest .",
    "docker:run": "docker run -p 3000:3000 backstop-backend:latest",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "deploy": "./deploy.sh start",
    "deploy:stop": "./deploy.sh stop",
    "deploy:restart": "./deploy.sh restart",
    "deploy:status": "./deploy.sh status",
    "deploy:logs": "./deploy.sh logs",
    "health": "curl -f http://localhost:3000/api/health",
    "backup": "./deploy.sh backup",
    "cleanup": "./deploy.sh cleanup"
  },
  "keywords": [
    "visual-regression",
    "backstopjs",
    "testing",
    "browser-isolation",
    "session-management",
    "docker",
    "puppeteer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.9.0",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "minimist": "^1.2.8",
    "multer": "^2.0.1",
    "puppeteer": "^24.10.1",
    "socket.io": "^4.7.2",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "backstopjs": "^6.3.25",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
